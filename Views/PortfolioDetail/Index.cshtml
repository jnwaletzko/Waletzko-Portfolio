@model IEnumerable<WaletzkoPortfolio.Models.PortfolioDetail>

@{
    ViewBag.Title = "Home";
}

@foreach (var item in Model) {
    <div class="row">
        <div class="col-lg-6">
            <h2>
                @Html.DisplayFor(modelItem => item.Description)
            </h2>
        </div>
        <div class="col-lg-6">
            @Html.DisplayFor(modelItem => item.Picture.PictureURL)
        </div>
    </div>
}
